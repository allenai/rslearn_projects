{
  "layers": {
    "era5": {
      "band_sets": [
        {
          "bands": [
            "2m-temperature",
            "total-precipitation"
          ],
          "dtype": "float32"
        }
      ],
      "data_source": {
        "class_path": "rslearn.data_sources.climate_data_store.ERA5LandMonthlyMeans",
        "query_config": {
          "max_matches": 12
        }
      },
      "type": "raster"
    },
    "label_raster": {
      "band_sets": [
        {
          "bands": [
            "label"
          ],
          "dtype": "int32"
        }
      ],
      "type": "raster"
    },
    "output": {
      "band_sets": [
        {
          "bands": [
            "output"
          ],
          "dtype": "float32"
        }
      ],
      "type": "raster"
    },
    "sentinel1": {
      "band_sets": [
        {
          "bands": [
            "vv",
            "vh"
          ],
          "dtype": "float32"
        }
      ],
      "data_source": {
        "class_path": "rslearn.data_sources.planetary_computer.Sentinel1",
        "ingest": false,
        "init_args": {
          "cache_dir": "cache/planetary_computer",
          "query": {
            "sar:instrument_mode": {
              "eq": "IW"
            },
            "sar:polarizations": {
              "eq": [
                "VV",
                "VH"
              ]
            }
          }
        },
        "query_config": {
          "max_matches": 12,
          "min_matches": 12,
          "period_duration": "30d",
          "space_mode": "PER_PERIOD_MOSAIC"
        }
      },
      "type": "raster"
    },
    "sentinel2": {
      "band_sets": [
        {
          "bands": [
            "B01",
            "B02",
            "B03",
            "B04",
            "B05",
            "B06",
            "B07",
            "B08",
            "B8A",
            "B09",
            "B11",
            "B12"
          ],
          "dtype": "uint16"
        }
      ],
      "data_source": {
        "class_path": "rslearn.data_sources.planetary_computer.Sentinel2",
        "ingest": false,
        "init_args": {
          "cache_dir": "cache/planetary_computer",
          "harmonize": true,
          "sort_by": "eo:cloud_cover"
        },
        "query_config": {
          "max_matches": 12,
          "min_matches": 12,
          "period_duration": "30d",
          "space_mode": "PER_PERIOD_MOSAIC"
        }
      },
      "type": "raster"
    }
  }
}
