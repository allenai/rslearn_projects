{
	"layers": {
		"naip": {
			"type": "raster",
			"band_sets": [{
				"dtype": "uint8",
				"bands": ["R", "G", "B"],
				"format": "png",
				"zoom_offset": 3
			}],
			"data_source": {
				"name": "rslearn.data_sources.aws_open_data.Naip",
				"index_cache_dir": "/mnt/data/cache/naip_index/",
				"use_rtree_index": true,
				"query_config": {
					"max_matches": 16
				}
			}
		}
	},
	"tile_store": {
		"name": "file",
		"root_dir": "/mnt/data/rslearn_superres_dataset/tiles",
		"default_raster_format": {
			"name": "custom"
		}
	}
}
