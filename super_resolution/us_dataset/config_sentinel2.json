{
	"layers": {
		"sentinel2": {
			"type": "raster",
			"band_sets": [{
				"dtype": "uint16",
				"bands": ["B02", "B03", "B04", "B08"],
				"format": "geotiff"
			}, {
				"dtype": "uint16",
				"bands": ["B05", "B06", "B07", "B8A", "B11", "B12"],
				"format": "geotiff",
				"zoom_offset": -1
			}, {
				"dtype": "uint16",
				"bands": ["B01", "B09", "B10"],
				"zoom_offset": -2
			}],
			"data_source": {
				"name": "rslearn.data_sources.aws_open_data.Sentinel2",
				"modality": "L1C",
				"metadata_cache_dir": "/mnt/superres_sentinel2/cache/sentinel2_metadata/",
				"max_time_delta": "1d",
				"query_config": {
					"max_matches": 4
				},
				"sort_by": "cloud_cover"
			}
		}
	},
	"tile_store": {
		"name": "file",
		"root_dir": "/mnt/superres_sentinel2/tiles"
	}
}
